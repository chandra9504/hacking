      $(function() {
        $("#form-1 input").iCheck({
          checkboxClass: "icheckbox_square-blue",
          increaseArea: "20%", // optional
        });
        
        var maxChecked = 3;
        $('#form-1 input').on('ifChecked', function(event){
        // Count how many checkboxes are currently checked
        var checkedBoxes = $('#form-1 input:checked').length;

        // If the maximum limit is reached, disable unchecked checkboxes
        if(checkedBoxes >= maxChecked){
          $('#form-1 input:not(:checked)').iCheck('disable');
        }
      });

      $('#form-1 input').on('ifUnchecked', function(event){
        // If any checkbox is unchecked, enable all checkboxes
        $('#form-1 input').iCheck('enable');
      });
      
        $("#form-2 input").iCheck({
